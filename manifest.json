{
  "manifest_version": 3,
  "name": "My New Tab",
  "version": "1.0.0",
  "description": "A beautiful and minimalist new tab page.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ]
}